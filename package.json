{
  "name": "renthunt",
  "author": "RentHunt Team",
  "description": "Mobile-first apartment rental platform with roommate compatibility matching",
  "keywords": [
    "react",
    "typescript",
    "vite",
    "mobile-first",
    "apartment-rental",
    "roommate-matching"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Onnson/renthunt"
  },
  "license": "MIT",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "setup": "git init && npx husky init && npx playwright install && shx rm .husky/pre-commit",
    "format": "prettier \"src/**/*.{ts,tsx}\" --write",
    "lint": "eslint --max-warnings 0",
    "lint:fix": "eslint --max-warnings 0 --fix",
    "dev": "vite",
    "test": "vitest run src/ && playwright test",
    "test:unit": "vitest src/",
    "test:unit:coverage": "vitest --coverage src/",
    "test:e2e": "playwright test",
    "test:e2e:report": "playwright show-report",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "commitlint": "commitlint --edit",
    "commitizen": "exec < /dev/tty && git cz --hook || true",
    "prepare": "husky"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.0",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slider": "^1.2.1",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toast": "^1.2.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.0.0",
    "lucide-react": "^0.468.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.54.0",
    "react-router-dom": "^7.9.4",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.32.0",
    "@faker-js/faker": "^9.9.0",
    "@hookform/devtools": "^4.4.0",
    "@playwright/test": "^1.54.1",
    "@tailwindcss/vite": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "@vitest/coverage-v8": "3.2.4",
    "autoprefixer": "^10.4.21",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unicorn": "^60.0.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "prop-types": "^15.8.1",
    "shx": "^0.4.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "7.0.6",
    "vite-plugin-static-copy": "^3.1.1",
    "vitest": "3.2.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
